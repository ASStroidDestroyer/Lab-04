#include <iostream>
using namespace std;

int main() {
    int num1 = 5, num2 = 3;
    cout << "Sum: " << num1 + num2 << endl;
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int num1 = 10, num2 = 3;
    cout << "Difference: " << num1 - num2 << endl;
    return 0;
}
